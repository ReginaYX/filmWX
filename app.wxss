/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
page{
  background: rgba(254,105,13,1);
}

.pageBg{
  border-radius: 20rpx 20rpx 0 0;
  background: #fff;
  height: 100vh;
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: -1;
}

.pageBox{
  box-sizing: border-box;
  border-radius: 20rpx 20rpx 0 0;
  padding: 55rpx 28rpx;
  min-height: 100vh;
}

.bottomBg{
  position: fixed;
  width: 100vw;
  bottom: 0;
  left: 0
}

/* 空状态 */
.nullBox{
  display: flex;
  align-items: center;
  flex-direction: column;
  font-size: 32rpx;
  color: #999
}

.nullBox>icon{
  font-size: 160rpx;
  margin: 30% 0 30rpx;
}

/* form表单 */
.formBox .inputBox{
  height: 114rpx;
  border-bottom: 2rpx solid rgba(151,151,151,0.2);
  display: flex;
  align-items: center;
  width: 100%;
}

.formBox .inputBox .inputTitle{
  color: #000;
  font-size: 30rpx;
  width: 22%;
  flex-shrink: 0;
}

.formBox .inputBox .inputTitle text{
  color: #F45258
}

.formBox .inputBox input{
  width: 330rpx;
  color: #000;
  font-size: 30rpx;
  flex-shrink: 0;
}

.formBox .inputBox .inputPlaceholder{
  color: #B2B2B2;
  font-size: 30rpx;
  width: 300rpx;
}

.formBox .inputBox .inputCode{
  font-size:30rpx;
  color:rgba(108,111,192,1);
  flex: 1;
  text-align: right;
}

.formBox .inputBox icon{
  color: #B1B5BD;
  font-size: 26rpx;
  flex: 1;
  text-align: right;
  margin: -16rpx 22rpx 0 0;
  transition: all 0.8s;
}

.formBox .inputBox icon.iconxiala{
  margin-top: -12rpx
}

.formBox button{
  height:86rpx;
  background:rgba(254,105,13,1);
  border-radius:60rpx;
  opacity:0.3;
  transition: all 0.8s;
  margin-top: 112rpx;
  font-size:30rpx;
  color: #fff;
  text-align: center;
  line-height: 86rpx;
}

.logForm button{
  background:rgba(0,0,0,1);
}

.formBox button.active{
  opacity: 1
}

.logForm button.active{
  background:rgba(254,105,13,1);
}

image.logo{
  width: 260rpx;
  margin: 0 auto;
  display: block;
}


